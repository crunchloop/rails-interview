<h1><%= @todo_item.title %></h1>
<p><%= @todo_item.description %></p>
<p>Completed: <%= @todo_item.completed? %></p>
<p>Created at: <%= @todo_item.created_at %></p>
<p>Updated at: <%= @todo_item.updated_at %></p>
<%= link_to "#{@todo_item.todo_list.name}", todo_list_path(@todo_item.todo_list), class: "btn btn-primary"%>
<br />
<%= link_to 'Edit', edit_todo_list_todo_item_path(@todo_item.todo_list, @todo_item), class: "btn btn-primary" %> |
<%= link_to 'Back', todo_list_todo_items_path(@todo_list), class: "btn btn-primary" %>
